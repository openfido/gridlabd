{
  "name" : "HiPAS GridLAB-D",
  "description" : "HiPAS GridLAB-D processing pipeline",
  "docker" : "lfenergy/arras:latest",
  "git" : "https://github.com/openfido/gridlabd.git",
  "branch" : "main",
  "script" : "openfido.sh",
  "manual" : {
    "gridlabd" : "rc"
  },
  "gridlabd" : {
    "GLOBALS" : {
      "description" : "Command line options, if any (e.g., -D name=value)",
      "prompt" : "Options",
      "default" : ""
    },
    "CONFIGCSV" : {
      "description" : "Input data files, if any (e.g., config.csv)",
      "prompt" : "Input files",
      "input_type" : "upload"
    },
    "MODELS" : {
      "description" : "Model files, if any (e.g., model.glm)",
      "prompt" : "Model GLM",
      "input_type" : "upload"
    },
    "TEMPLATE" : {
      "description" : "List analysis template, if any (e.g., -t template). See https://template.gridlabd.us/README.md.",
      "prompt" : "Analysis template",
      "default" : ""
    }
  }
}
